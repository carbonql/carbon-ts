{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    // "strictNullChecks": true,

    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "allowJs": true,
    "outDir": "build",
    "sourceMap": true
  },
  "exclude": [
    "node_modules/@kubernetes/client-node",
    "guestbook",
    "build"
  ]
}